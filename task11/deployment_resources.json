{
  "lambda-basic-execution": {
    "policy_content": {
      "Statement": [
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents",
            "dynamodb:GetItem",
            "dynamodb:Query",
            "dynamodb:PutItem",
            "dynamodb:Batch*",
            "dynamodb:DeleteItem",
            "dynamodb:Scan",
            "ssm:PutParameter",
            "ssm:GetParameter",
            "kms:Decrypt",
            "cognito-identity:*",
            "cognito-idp:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy",
    "tags": {}
  },
  "simple-booking-userpool": {
    "resource_type": "cognito_idp",
    "password_policy": {
      "minimum_length": 8,
      "require_uppercase": true,
      "require_symbols": true,
      "require_lowercase": true,
      "require_numbers": true
    },
    "auto_verified_attributes": [],
    "sms_configuration": {},
    "username_attributes": [],
    "custom_attributes": [
      {
        "name": "firstName",
        "type": "String"
      },
      {
        "name": "lastName",
        "type": "String"
      },
      {
        "name": "email",
        "type": "String"
      }
    ],
    "client": {
      "client_name": "client-app",
      "generate_secret": false,
      "explicit_auth_flows": [
        "ALLOW_ADMIN_USER_PASSWORD_AUTH",
        "ALLOW_REFRESH_TOKEN_AUTH"
      ]
    },
    "tags": {}
  },
