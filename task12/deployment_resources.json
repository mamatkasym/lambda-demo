{
  "lambda-basic-execution": {
    "policy_content": {
      "Statement": [
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents",
            "dynamodb:GetItem",
            "dynamodb:Query",
            "dynamodb:PutItem",
            "dynamodb:Batch*",
            "dynamodb:DeleteItem",
            "dynamodb:Scan",
            "ssm:PutParameter",
            "ssm:GetParameter",
            "kms:Decrypt",
            "cognito-identity:*",
            "cognito-idp:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy",
    "tags": {}
  },
  "simple-booking-userpool": {
    "resource_type": "cognito_idp",
    "password_policy": {
      "minimum_length": 8,
      "require_uppercase": true,
      "require_symbols": true,
      "require_lowercase": true,
      "require_numbers": true
    },
    "auto_verified_attributes": [],
    "sms_configuration": {},
    "username_attributes": [],
    "custom_attributes": [
      {
        "name": "firstName",
        "type": "String"
      },
      {
        "name": "lastName",
        "type": "String"
      },
      {
        "name": "email",
        "type": "String"
      }
    ],
    "client": {
      "client_name": "client-app",
      "generate_secret": false,
      "explicit_auth_flows": [
        "ALLOW_ADMIN_USER_PASSWORD_AUTH",
        "ALLOW_REFRESH_TOKEN_AUTH"
      ]
    },
    "tags": {}
  },
  "Tables": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "id",
    "hash_key_type": "N",
    "read_capacity": 1,
    "write_capacity": 1,
    "global_indexes": [],
    "autoscaling": [],
    "tags": {}
  },
  "Reservations": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "id",
    "hash_key_type": "S",
    "read_capacity": 1,
    "write_capacity": 1,
    "global_indexes": [],
    "autoscaling": [],
    "tags": {}
  },
  "${ui-bucket}": {
    "policy":     {

        "Version": "2012-10-17",

        "Statement": [

            {

                "Effect": "Allow",

                "Principal": "*",

                "Action": ["s3:GetObject", "s3:PutObject", "s3:*"],

                "Resource": ["arn:aws:s3:::${ui-bucket}/*"],

                "Condition": {

                    "IpAddress": {

                        "aws:SourceIp": "174.128.60.160/32"

                    }

                }

            }

        ]

    }
,
    "resource_type": "s3_bucket",
    "acl": "private",
    "cors": [],
    "tags": {}
  }
}